    hpicfPimWildcardEntry OBJECT-TYPE
        SYNTAX      TruthValue
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION "Specifies wildcard routing entries."
        ::= { hpicfPimMRouteEntry 11 }

    hpicfPimRPFPrimeNeighborAddressType OBJECT-TYPE
