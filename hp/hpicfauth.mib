        SYNTAX      OCTET STRING (SIZE(0..1024))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION "The SHA256 hash-type of the Manager Password."
        ::= { hpicfSwitchUserConfigEntry 21 }

