            INTEGER,
        hpnicfOnuRS485DataBits
            INTEGER,
        hpnicfOnuRS485Parity
            INTEGER,
        hpnicfOnuRS485StopBits
            INTEGER,
        hpnicfOnuRS485FlowControl
            INTEGER,
        hpnicfOnuRS485TXOctets
            Integer32,
        hpnicfOnuRS485RXOctets
            Integer32,
        hpnicfOnuRS485TXErrOctets
            Integer32,
