    INDEX
        {
            ifIndex
        }
    ::= { hpnicfEponStatTable 1 }

HpnicfEponStatEntry ::= SEQUENCE
    {
        hpnicfEponStatFER    Counter64,
        hpnicfEponStatBER    Counter64
    }

