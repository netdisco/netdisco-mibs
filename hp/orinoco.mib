oriSyslogHostTableEntryStatus OBJECT-TYPE
    SYNTAX INTEGER
  {
      enable(1),
      disable(2),
      delete(3),
      create(4)
  }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
