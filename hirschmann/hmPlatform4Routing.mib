    hmPlatform4                         FROM HIRSCHMANN-MMP4-BASICL2-MIB;


    hmPlatform4Routing MODULE-IDENTITY
       LAST-UPDATED "200508181200Z" -- 18 Aug 2005 12:00:00 GMT
        ORGANIZATION "Hirschmann Automation and Control GmbH"
        CONTACT-INFO
