        ::= { h3cIPSecSaEntry 5 }

    h3cIPSecSaAuthAlgo OBJECT-TYPE
        SYNTAX H3cAuthAlgo
        MAX-ACCESS read-only
        STATUS current
        DESCRIPTION
            "The authentication algorithm used by the SA."
        ::= { h3cIPSecSaEntry 6 }

    h3cIPSecSaStatus OBJECT-TYPE
