        MODULE -- this module
        MANDATORY-GROUPS
            {
                h3cIPSecTunnelTableGroup,
                h3cIPSecTunnelStatGroup,
                h3cIPSecSaGroup,
                h3cIPSecTrafficTableGroup,
                h3cIPSecGlobalStatsGroup,
                h3cIPSecTrapObjectGroup,
                h3cIPSecTrapCntlGroup,
                h3cIPSecTrapGroup
            }
        ::= { h3cIPSecCompliances 1 }

