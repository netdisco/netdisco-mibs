IMPORTS
    enterprises                                 FROM RFC1155-SMI
    MODULE-IDENTITY, OBJECT-TYPE, Integer32,
    Unsigned32, Counter32, Counter64, IpAddress FROM SNMPv2-SMI
    DisplayString, MacAddress, TruthValue       FROM SNMPv2-TC;

digiEthMIB MODULE-IDENTITY
	LAST-UPDATED "201605250000Z"	-- 25 May 2016
	ORGANIZATION
		"Digi International, Inc."
	CONTACT-INFO
		"Digi International Tech Support

		 Phone: 877 912 3444     (US and Canada)
		 		+1 952 912 3456  (Worldwide)
		 Email: <Tech.Support@digi.com>"
	DESCRIPTION
		"The Digi Ethernet MIB for managing the Ethernet statistics on the 
		 Digi TransPort product range."
	::= { sarian 10004 }


sarian         OBJECT IDENTIFIER ::= { enterprises 16378 }
sarian-eth     OBJECT IDENTIFIER ::= { sarian 10004 }
eth-stats      OBJECT IDENTIFIER ::= { sarian-eth 1 }

--
-- Eth Table
--
ethTable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF EthEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
         "A table containing statistics information on a per
          Ethernet interface basis."
    ::= { eth-stats 1 }

ethEntry   OBJECT-TYPE
    SYNTAX      EthEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
             "An entry containing statistics for a particular Ethernet interface."
    INDEX { ethIndex }
    ::= { ethTable 1 }

EthEntry ::=
    SEQUENCE{
    ethIndex     INTEGER,
    }

ethIndex   OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
         "Index into the ETH table"
    ::= { ethEntry 1 }

ethTxBytes   OBJECT-TYPE
	SYNTAX      Counter32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"The number of bytes transmitted."
	::= { ethEntry 2 }

ethRxBytes   OBJECT-TYPE
	SYNTAX      Counter32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
