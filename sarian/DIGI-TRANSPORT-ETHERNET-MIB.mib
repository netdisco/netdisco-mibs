	::= { ethEntry 3 }

ethTxPackets   OBJECT-TYPE
	SYNTAX      Counter32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
		"The number of packets transmitted."
	::= { ethEntry 4 }

ethTxUnicastPackets   OBJECT-TYPE
	SYNTAX      Counter32
	MAX-ACCESS  read-only
	STATUS      current
	DESCRIPTION
