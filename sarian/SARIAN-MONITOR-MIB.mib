	Counter64, IpAddress,Integer32				FROM SNMPv2-SMI
	MODULE-COMPLIANCE, OBJECT-GROUP				FROM SNMPv2-CONF
	DisplayString								FROM SNMPv2-TC;

sarianMonitorMIB MODULE-IDENTITY
	LAST-UPDATED "201305280000Z"	-- 28 May 2013
	ORGANIZATION
		"Digi International, Inc."
	CONTACT-INFO
		"Digi International Tech Support

		 Phone: 877 912 3444     (US and Canada)
		 		+1 952 912 3456  (Worldwide)
		 EMail: <Tech.Support@digi.com>"
	DESCRIPTION
		"The Sarian Monitor MIB for managing the Digi TransPort product range."
	REVISION	"201305280000Z"	-- 28 May 2013
	DESCRIPTION
		"Updated to comply with SMI-v2 syntax definitions."
	::= { sarian 11000 }

sarian			OBJECT IDENTIFIER ::= { enterprises 16378 }
sarianMonitor	OBJECT IDENTIFIER ::= { sarian 10000 }

sarianPPP		OBJECT IDENTIFIER ::= { sarianMonitor 1 }
sarianGPRS		OBJECT IDENTIFIER ::= { sarianMonitor 2 }
sarianSystem	OBJECT IDENTIFIER ::= { sarianMonitor 3 }
sarianConfig	OBJECT IDENTIFIER ::= { sarianMonitor 4 }
sarianCDMA		OBJECT IDENTIFIER ::= { sarianMonitor 5 }


--
-- Sarian PPP
--
numPPPInterfaces	OBJECT-TYPE
	SYNTAX		Integer32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The number of PPP interfaces."
	::= { sarianPPP 1 }

sarianPPPTable	OBJECT-TYPE
	SYNTAX		SEQUENCE OF SarianPPPEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"A list of entries containing status information for all
		PPP entities on this system."
	::= { sarianPPP 2 }

sarianPPPEntry	OBJECT-TYPE
	SYNTAX		SarianPPPEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"An entry containing status information for a particular
		PPP entity."
	INDEX { ifIndex }
	::= { sarianPPPTable 1 }

SarianPPPEntry ::=
	SEQUENCE {
		ifIndex			  	Integer32,
		pppDLTotalData	  	Counter64,
		pppDLWarn		  	Counter64,
		pppDLStop		  	Counter64,
		pppDLDayReset	  	Integer32,
		pppDLLastReset	  	DisplayString,
		pppDLResetTotalData	Integer32,
		pppUserName	  	    DisplayString,
		pppUptime	Integer32
}

ifIndex 		OBJECT-TYPE
	SYNTAX	Integer32 (0..2147483647)
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"A unique value assigned to each interface."
	::= { sarianPPPEntry 1 }

pppDLTotalData	OBJECT-TYPE
	SYNTAX	Counter64
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"Total amount of data (in bytes) transferred in this PPP
		entity during the data limit period."
	::= { sarianPPPEntry 2 }

pppDLWarn	OBJECT-TYPE
	SYNTAX	Counter64
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The PPP data limit (in bytes) at which point a warning
		event is generated."
	::= { sarianPPPEntry 3 }

pppDLStop	OBJECT-TYPE
	SYNTAX	Counter64
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The PPP data limit (in bytes) at which point no more
		traffic is transferred."
	::= { sarianPPPEntry 4 }

pppDLDayReset	OBJECT-TYPE
	SYNTAX	Integer32 (0..31)
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"The day of the month on which the total data transferred 
		is reset. If this object is zero, the total data 
		transferred needs to be manually reset."
	::= { sarianPPPEntry 5 }

pppDLLastReset	OBJECT-TYPE
	SYNTAX	DisplayString
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION
		"The data on which the last reset of the total data
		transferred was reset."
	::= { sarianPPPEntry 6 }

pppDLResetTotalData	OBJECT-TYPE
	SYNTAX	Integer32
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"Writing to this object will reset the total data
		counter. Reading this value will always return 0."
	::= { sarianPPPEntry 7 }

pppUserName	OBJECT-TYPE
	SYNTAX	DisplayString
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"The username used during authentication."
	::= { sarianPPPEntry 8 }

pppUptime	OBJECT-TYPE
	SYNTAX	Integer32
	MAX-ACCESS	read-write
	STATUS	current
	DESCRIPTION
		"The time in seconds that this connection has been up."
	::= { sarianPPPEntry 9 }

