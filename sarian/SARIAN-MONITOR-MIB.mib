-- Conformance Information
sarianMonitorConformance	OBJECT IDENTIFIER ::= { sarianMonitor 100 }

sarianMonitorGroups			OBJECT IDENTIFIER ::= { sarianMonitorConformance 1 }
sarianMonitorCompliances	OBJECT IDENTIFIER ::= { sarianMonitorConformance 2 }

sarianMonitorCompliance MODULE-COMPLIANCE
	STATUS	current
	DESCRIPTION
			"Compliance statement for Sarian Monitor objects."

	MODULE
		GROUP			sarianPPPGroup
		DESCRIPTION
				"Compliance group for PPP objects"
				
		GROUP			sarianGPRSGroup
		DESCRIPTION
				"Compliance group for GPRS objects"
				
		GROUP			sarianSystemGroup
		DESCRIPTION
				"Compliance group for system objects"
				
		GROUP			sarianConfigGroup
		DESCRIPTION
				"Compliance group for configuration objects"
				
		GROUP			sarianCDMAGroup
		DESCRIPTION
				"Compliance group for CDMA objects"

	::= { sarianMonitorCompliances 1 }

sarianMonitorCompliance2 MODULE-COMPLIANCE
	STATUS	deprecated
	DESCRIPTION
			"Compliance statement for deprecated Sarian Monitor objects."

	MODULE
		GROUP			sarianGPRSCDMAGroup
		DESCRIPTION
				"Compliance group for deprecated CDMA objects"
				
	::= { sarianMonitorCompliances 2 }


sarianPPPGroup	OBJECT-GROUP
	OBJECTS { numPPPInterfaces, ifIndex, pppDLTotalData, pppDLWarn, pppDLStop,
				pppDLDayReset, pppDLLastReset, pppDLResetTotalData }
	STATUS	current
	DESCRIPTION
			"A collection of objects for monitoring PPP interfaces."
	::= { sarianMonitorGroups 1 }

sarianGPRSGroup OBJECT-GROUP
	OBJECTS { gprsSignalStrength, gprsRegistered, gprsAttached,
				gprsPPPIP, gprsAreaID, gprsCellID, gprsNetworkTechnology }
	STATUS	current
	DESCRIPTION
			"A collection of objects for monitoring GPRS connections."
	::= { sarianMonitorGroups 2 }

sarianGPRSCDMAGroup OBJECT-GROUP
	OBJECTS { gprsCdmaEcio, gprsCdmaPilotPN, gprsCdmaDRC, gprsCdmaESN, 
				gprsCdmaMEID, gprsCdmaMDN, gprsCdmaMIN, 
				gprsCdmaSignalStrength, gprsEvdoSignalStrength, 
				gprsEvdoEcio }
	STATUS	deprecated
	DESCRIPTION
			"A collection of objects for monitoring CDMA connections."
	::= { sarianMonitorGroups 3 }


sarianSystemGroup OBJECT-GROUP
	OBJECTS { cpuUtil, cpuMin, cpuMax, cpuAvg, msgsFree, msgsMin, 
				bufsFree, bufsMin, bigBufsFree, bigBufsMin, temperature }
	STATUS	current
	DESCRIPTION
			"A collection of objects for monitoring system information."
	::= { sarianMonitorGroups 4 }

sarianConfigGroup OBJECT-GROUP
	OBJECTS { changesSincePowerUp, unsavedChanges, lastSavedConfigUser, 
				lastSavedConfigTime, lastSavedConfigNumber, 
				pwdChangesSincePowerUp, unsavedPwdChanges, 
				lastSavedPwdConfigUser, lastSavedPwdConfigTime, 
				lastSavedPwdConfigNumber }
	STATUS	current
	DESCRIPTION
			"A collection of objects for monitoring configuration information."
	::= { sarianMonitorGroups 5 }

sarianCDMAGroup OBJECT-GROUP
	OBJECTS { cdmaServiceMode, cdmaPilotPN, cdmaDRC, cdmaESN, cdmaMEID, 
				cdmaMDN, cdmaMIN, cdmaPRLVersion, cdmaSignalStrength, 
				evdoSignalStrength, cdmaEcio, evdoEcio }
	STATUS	current
	DESCRIPTION
			"A collection of objects for monitoring CDMA connections."
	::= { sarianMonitorGroups 6 }

END
