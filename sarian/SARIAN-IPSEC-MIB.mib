IMPORTS
    enterprises                                 FROM RFC1155-SMI
    MODULE-IDENTITY, OBJECT-TYPE, Integer32,
    Unsigned32, Counter32, Counter64, IpAddress FROM SNMPv2-SMI
    DisplayString, MacAddress, TruthValue       FROM SNMPv2-TC;

sarianIpsecMIB MODULE-IDENTITY
	LAST-UPDATED "201606010000Z"	-- 1 Jun 2016
	ORGANIZATION
		"Digi International, Inc."
	CONTACT-INFO
		"Digi International Tech Support

		 Phone: 877 912 3444     (US and Canada)
		 		+1 952 912 3456  (Worldwide)
		 EMail: <Tech.Support@digi.com>"
	DESCRIPTION
		"The Sarian IPsec MIB for managing the IPsec SAs on the 
		 Digi TransPort product range."
	::= { sarian 10005 }


sarian           OBJECT IDENTIFIER ::= { enterprises 16378 }
sarian-ipsec     OBJECT IDENTIFIER ::= { sarian 10005 }

ipsec-sas 					OBJECT IDENTIFIER ::= { sarian-ipsec 1 }

ipsec-sas-v1     			OBJECT IDENTIFIER ::= { ipsec-sas 1 }

ipsec-sas-v2                OBJECT IDENTIFIER ::= { ipsec-sas 2 }

--
-- IPsec V1 SAs Table
--
IPsecV1InboundSATable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF V1InboundSAEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
         "A list of entries containing information on all
         IPsec V1 inbound SAs."
    ::= { ipsec-sas-v1 1 }

IPsecV1InboundSAEntry   OBJECT-TYPE
    SYNTAX      V1InboundSAEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
         "An entry containing information for a particular
         inbound IPsec V1 SA."
    INDEX { ipsecV1InboundSaEroute }
    ::= { IPsecV1InboundSATable 1 }

IPsecV1OutboundSATable   OBJECT-TYPE
    SYNTAX      SEQUENCE OF V1OutboundSAEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
         "A list of entries containing information on all
         IPsec V1 outbound SAs."
    ::= { ipsec-sas-v1 2 }

IPsecV1OutboundSAEntry   OBJECT-TYPE
    SYNTAX      V1OutboundSAEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
         "An entry containing information for a particular
         outbound IPsec V1 SA."
    INDEX { ipsecV1OutboundSaEroute }
    ::= { IPsecV1OutboundSATable 1 }

V1InboundSAEntry ::=
        SEQUENCE {
                ipsecV1InboundSaSpi          Unsigned32,
                ipsecV1InboundSaEroute       Unsigned32,
                ipsecV1InboundSaPeerId       DisplayString,
				ipsecV1InboundSaPeerIp       IpAddress,
				ipsecV1InboundSaLocalIp      IpAddress,
				ipsecV1InboundSaLocalMask    IpAddress,
				ipsecV1InboundSaRemoteIp     IpAddress,
				ipsecV1InboundSaRemoteMask   IpAddress,
                ipsecV1InboundSaTtl          Unsigned32,
                ipsecV1InboundSaKiBRemaining Unsigned32
        }

V1OutboundSAEntry ::=
        SEQUENCE {
                ipsecV1OutboundSaSpi          Unsigned32,
                ipsecV1OutboundSaEroute       Unsigned32,
                ipsecV1OutboundSaPeerId       DisplayString,
				ipsecV1OutboundSaPeerIp       IpAddress,
				ipsecV1OutboundSaLocalIp      IpAddress,
				ipsecV1OutboundSaLocalMask    IpAddress,
				ipsecV1OutboundSaRemoteIp     IpAddress,
				ipsecV1OutboundSaRemoteMask   IpAddress,
                ipsecV1OutboundSaTtl          Unsigned32,
                ipsecV1OutboundSaKiBRemaining Unsigned32
        }

