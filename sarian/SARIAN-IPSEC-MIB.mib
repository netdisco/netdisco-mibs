    ::= { IPsecV1InboundSAEntry 8 }

ipsecV1InboundSaTtl OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
         "The time to live in seconds for this SA."
    ::= { IPsecV1InboundSAEntry 9 }

ipsecV1InboundSaKiBRemaining OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
         "The number of Kibibytes remaining for this SA."
    ::= { IPsecV1InboundSAEntry 10 }

