		 Email: <Tech.Support@digi.com>"
	DESCRIPTION
		"The Digi Services MIB for managing the Services configuration on the 
		 Digi TransPort product range."
	::= { sarian 10002 }


sarian OBJECT IDENTIFIER ::= { enterprises 16378 }
digiServices OBJECT IDENTIFIER ::= { sarian 10002 }
services-config OBJECT IDENTIFIER ::= { digiServices 1 }

