 		::= { wwpLeosInformStatusEntry 14} 
 		
 wwpLeosInformStatusResend		OBJECT-TYPE
 		SYNTAX INTEGER {
 				none(0),
 				send(1)
 		}				
 		MAX-ACCESS read-write
