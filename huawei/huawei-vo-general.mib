				{
				fxs2(1),
				fxo2(2),
				em2(3),
				fxs4(4),
				fxo4(5),
				em4(6),
				e1vi(7),
				t1vi(8),
				sic-fxs1(9),
				sic-fxo1(10),
				sic-fxs2(11),
				sic-fxo2(12)
				}
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The board type of the physical voice channel."
			::= { hwVoLtoPEntry 6 }

		
		hwVoLtoPPortNumber OBJECT-TYPE
			SYNTAX INTEGER
			MAX-ACCESS read-only
			STATUS current
			DESCRIPTION
				"The global port number of the logic voice channel.
				-1 represent this channel cann't be used by voice."
