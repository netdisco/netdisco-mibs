    MAX-ACCESS   read-write
    STATUS       current
    DESCRIPTION
        "This object represents the UBP filter name of the fail-open policy"
    ::= { bseePortConfigEntry 27 }
		
bseePortConfigCloneSettingsAction OBJECT-TYPE
