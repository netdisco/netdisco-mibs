                    led-off                 (1) , 
                    led-green               (2) , 
                    led-red                 (3) , 
                    led-yellow              (4) , 
                    led-blinking-green      (5) , 
                    led-blinking-red        (6) , 
                    led-blinking-yellow     (7)
                }   

