	::= { nngenHdlcStatsTableEntry 9 }

nngenHdlcStatsPktsTxLastFiveMins	OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION 
