	DESCRIPTION 
		""
	::= { nngenHdlcStatsTableEntry 3 }

nngenHdlcStatsPktsTxLastBootClear	OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION 
		""
	::= { nngenHdlcStatsTableEntry 4 }

nngenHdlcStatsErrPktsRxLastBootClear	OBJECT-TYPE
