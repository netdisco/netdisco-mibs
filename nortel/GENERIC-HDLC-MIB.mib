	::= { nngenHdlcStatsTableEntry 5 }

nngenHdlcStatsUpDownStatesLastBootClear	OBJECT-TYPE
	SYNTAX	Counter32
	MAX-ACCESS	read-only
	STATUS	current
	DESCRIPTION 
		""
	::= { nngenHdlcStatsTableEntry 6 }

nngenHdlcStatsBytesRxLastFiveMins	OBJECT-TYPE
