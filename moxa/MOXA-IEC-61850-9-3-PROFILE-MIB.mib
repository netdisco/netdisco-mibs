    iec6185093StatClock  OBJECT IDENTIFIER
        ::= { iec6185093Status 1 }

    iec6185093StatClockCurrentDSTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF Iec6185093StatClockCurrentDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "Table of information about the PTP clock currentDS for
                 all domains."
        ::= { iec6185093StatClock 1 }

    iec6185093StatClockCurrentDSEntry OBJECT-TYPE
        SYNTAX          Iec6185093StatClockCurrentDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "A table entry that contains information about a single
                 PTP clock currentDS for a domain."
        REFERENCE
                "Section 8.2.2 ('currentDS data set member
                 specifications') of [IEEE-1588-2008]"
        INDEX { iec6185093StatClockCurrentDSInstanceIndex }
        ::= { iec6185093StatClockCurrentDSTable 1 }

    Iec6185093StatClockCurrentDSEntry ::=
        SEQUENCE {
            iec6185093StatClockCurrentDSInstanceIndex    PtpClockInstanceType,
            iec6185093StatClockCurrentDSStepsRemoved     Unsigned32,
            iec6185093StatClockCurrentDSOffsetFromMaster PtpClockTimeInterval,
            iec6185093StatClockCurrentDSMeanPathDelay    PtpClockTimeInterval,
            iec6185093StatClockCurrentDSSyncStatus       Unsigned32,
            iec6185093StatClockCurrentDSSlavePort        Integer32,
            iec6185093StatClockCurrentDSClockTime        DisplayString
        }

    iec6185093StatClockCurrentDSInstanceIndex OBJECT-TYPE
        SYNTAX          PtpClockInstanceType
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "This object specifies the instance of the clock for this clock
                 type in the given domain."
        ::= { iec6185093StatClockCurrentDSEntry 1 }

    iec6185093StatClockCurrentDSStepsRemoved OBJECT-TYPE
        SYNTAX          Unsigned32
        UNITS           "Steps"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "The current clock dataset stepsRemoved value.

                 This object specifies the distance measured by the number of
                 boundary clocks between the local clock and the foreign master
                 as indicated in the stepsRemoved field of Announce messages."
        REFERENCE
                "Section 8.2.2.2 ('stepsRemoved') of [IEEE-1588-2008]"
        ::= { iec6185093StatClockCurrentDSEntry 2 }

    iec6185093StatClockCurrentDSOffsetFromMaster OBJECT-TYPE
        SYNTAX          PtpClockTimeInterval
        UNITS           "Time Interval"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the current clock dataset ClockOffset
                 value.  The value of the computation of the offset in time
                 between a slave and a master clock."
        REFERENCE
                "Section 8.2.2.3 ('currentDS.offsetFromMaster')
                 of [IEEE-1588-2008]"
        ::= { iec6185093StatClockCurrentDSEntry 3 }

    iec6185093StatClockCurrentDSMeanPathDelay OBJECT-TYPE
        SYNTAX          PtpClockTimeInterval
        UNITS           "Time Interval"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the current clock dataset
                 MeanPathDelay value.

                 The mean path delay between a pair of ports as measured by the
                 delay request-response mechanism."
        REFERENCE
                "Section 8.2.2.4 ('currentDS.meanPathDelay')
                 of [IEEE-1588-2008]"
        ::= { iec6185093StatClockCurrentDSEntry 4 }


    iec6185093StatClockCurrentDSSyncStatus OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "A value of 'DISABLED' (0) indicates that the ptp function is 
                 disabled. A value of 'FREERUN' (1) indicates that the device
                 is selected to be master. A value of 'SYNCING' (2) indicates
                 that the device is selected to be slave and is synchronizing
                 to grandmaster. A value of 'LOCKED' (3) indicates that the
                 device is synchronized to the grandmaster. A value of
                 'INITIALIZING' (4) indicates that the ptp function is
                 initializing."
        DEFVAL { 1 }
        ::= { iec6185093StatClockCurrentDSEntry 5 }

    iec6185093StatClockCurrentDSSlavePort OBJECT-TYPE
        SYNTAX          Integer32(1..255)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "Indicate the port which is in the SLAVE state."
        ::= { iec6185093StatClockCurrentDSEntry 6 }

    iec6185093StatClockCurrentDSClockTime OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "Indicate the PTP Clock Time."
        ::= { iec6185093StatClockCurrentDSEntry 7 }

    iec6185093StatClockParentDSTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF Iec6185093StatClockParentDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "Table of information about the PTP clock parentDS for
                 all domains."
        ::= { iec6185093StatClock 2 }

    iec6185093StatClockParentDSEntry OBJECT-TYPE
        SYNTAX          Iec6185093StatClockParentDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "A table entry that contains information about a single
                 PTP clock parentDS for a domain."
        REFERENCE
                "Section 8.2.3 ('parentDS data set member specifications') of
                 [IEEE-1588-2008]"
        INDEX { iec6185093StatClockParentDSInstanceIndex }
        ::= { iec6185093StatClockParentDSTable 1 }

    Iec6185093StatClockParentDSEntry ::=
        SEQUENCE {
            iec6185093StatClockParentDSInstanceIndex          PtpClockInstanceType,
            iec6185093StatClockParentDSParentPortIdentity     OCTET STRING,
            iec6185093StatClockParentDSGMClockIdentity        PtpClockIdentity,
            iec6185093StatClockParentDSGMClockPriority1       Unsigned32,
            iec6185093StatClockParentDSGMClockPriority2       Unsigned32,
            iec6185093StatClockParentDSGMClockQualityClass    PtpClockQualityClassType,
            iec6185093StatClockParentDSGMClockQualityAccuracy PtpClockQualityAccuracyType,
            iec6185093StatClockParentDSParentPortNumber       PtpClockPortNumber
        }

    iec6185093StatClockParentDSInstanceIndex OBJECT-TYPE
        SYNTAX          PtpClockInstanceType
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "This object specifies the instance of the clock for this clock
                 type in the given domain."
        ::= { iec6185093StatClockParentDSEntry 1 }

    iec6185093StatClockParentDSParentPortIdentity OBJECT-TYPE
        SYNTAX          OCTET STRING(SIZE(1..256))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the value of portIdentity of the port on
                 the master that issues the Sync messages used in synchronizing
                 this clock."
        REFERENCE
                "Section 8.2.3.2 ('parentDS.parentPortIdentity') of
                 [IEEE-1588-2008]"
        ::= { iec6185093StatClockParentDSEntry 2 }

    iec6185093StatClockParentDSGMClockIdentity OBJECT-TYPE
        SYNTAX          PtpClockIdentity
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 identity."
        REFERENCE
                "Section 8.2.3.6 ('parentDS.grandmasterIdentity') of
                 [IEEE-1588-2008]"
        ::= { iec6185093StatClockParentDSEntry 3 }

    iec6185093StatClockParentDSGMClockPriority1 OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 priority1."
        REFERENCE
                "Section 8.2.3.8 ('parentDS.grandmasterPriority1') of
                 [IEEE-1588-2008]"
        ::= { iec6185093StatClockParentDSEntry 4 }

    iec6185093StatClockParentDSGMClockPriority2 OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 priority2."
        REFERENCE
                "Section 8.2.3.9 ('parentDS.grandmasterPriority2') of
                 [IEEE-1588-2008]"
        ::= { iec6185093StatClockParentDSEntry 5 }

    iec6185093StatClockParentDSGMClockQualityClass OBJECT-TYPE
        SYNTAX          PtpClockQualityClassType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 quality class."
        REFERENCE
                "Section 8.2.3.7 ('parentDS.grandmasterClockQuality') of
                 [IEEE-1588-2008]"
        ::= { iec6185093StatClockParentDSEntry 6 }

    iec6185093StatClockParentDSGMClockQualityAccuracy OBJECT-TYPE
        SYNTAX          PtpClockQualityAccuracyType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 quality accuracy."
        REFERENCE
                "Section 8.2.3.7 ('parentDS.grandmasterClockQuality') of
                 [IEEE-1588-2008]"
        ::= { iec6185093StatClockParentDSEntry 7 }

    iec6185093StatClockParentDSParentPortNumber OBJECT-TYPE
        SYNTAX          PtpClockPortNumber
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS PTP portNumber"
        ::= { iec6185093StatClockParentDSEntry 8 }

    iec6185093StatClockPortRunningTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF Iec6185093StatClockPortRunningEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "Table of information about the clock ports running datasets for
                 a particular domain."
        ::= { iec6185093StatClock 3 }

    iec6185093StatClockPortRunningEntry OBJECT-TYPE
        SYNTAX          Iec6185093StatClockPortRunningEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "A table entry that contains running dataset information
                 about a single clock port."
        INDEX { iec6185093StatClockPortRunningPortNumberIndex }
        ::= { iec6185093StatClockPortRunningTable 1 }

    Iec6185093StatClockPortRunningEntry ::=
        SEQUENCE {
            iec6185093StatClockPortRunningPortNumberIndex    PtpClockPortNumber,
            iec6185093StatClockPortRunningState              INTEGER,
            iec6185093StatClockPortRunningPeerMeanPathDelay  PtpClockTimeInterval
        }

    iec6185093StatClockPortRunningPortNumberIndex OBJECT-TYPE
        SYNTAX          PtpClockPortNumber
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "This object specifies the PTP portNumber associated with this
                 clock port."
        ::= { iec6185093StatClockPortRunningEntry 1 }

    iec6185093StatClockPortRunningState OBJECT-TYPE
        SYNTAX          INTEGER  {
                            initializing(1),
                            faulty(2),
                            disabled(3),
                            listening(4),
                            preMaster(5),
                            master(6),
                            passive(7),
                            uncalibrated(8),
                            slave(9),
                            TransmittingSynchronized(10),
                            ReceivingSynchronized (11)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the port state returned by PTP engine:

                initializing
                faulty
                disabled
                listening
                preMaster
                master
                passive
                uncalibrated
                slave
                TransmittingSynchronized
                ReceivingSynchronized       "
        ::= { iec6185093StatClockPortRunningEntry 2 }

    iec6185093StatClockPortRunningPeerMeanPathDelay OBJECT-TYPE
        SYNTAX          PtpClockTimeInterval
        UNITS           "Time Interval"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the peer meanPathDelay."
        ::= { iec6185093StatClockPortRunningEntry 3 }

END
