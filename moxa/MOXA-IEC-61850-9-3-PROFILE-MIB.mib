-----------------------
-- IEC 61850-9-3 2016 -- 
-----------------------
    iec6185093ConfigClock  OBJECT IDENTIFIER
        ::= { iec6185093Configuration 1 }

    iec6185093ConfigClockDefaultDSTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF Iec6185093ConfigClockDefaultDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "Table of information about the PTP clock defaultDS for
                 all domains."
        ::= { iec6185093ConfigClock 1 }

    iec6185093ConfigClockDefaultDSEntry OBJECT-TYPE
        SYNTAX          Iec6185093ConfigClockDefaultDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "A table entry that contains information about a single
                 PTP clock defaultDS for a domain."
        INDEX  { iec6185093ConfigClockDefaultDSInstanceIndex }
        ::= { iec6185093ConfigClockDefaultDSTable 1 }

    Iec6185093ConfigClockDefaultDSEntry ::= 
        SEQUENCE {
            iec6185093ConfigClockDefaultDSInstanceIndex       PtpClockInstanceType,
            iec6185093ConfigClockDefaultDSClockType           PtpClockType,
            iec6185093ConfigClockDefaultDSDomainNumber        PtpClockDomainType,
            iec6185093ConfigClockDefaultDSTwoStepFlag         TruthValue,
            iec6185093ConfigClockDefaultDSClockIdentity       PtpClockIdentity,
            iec6185093ConfigClockDefaultDSPriority1           Unsigned32,
            iec6185093ConfigClockDefaultDSPriority2           Unsigned32,
            iec6185093ConfigClockDefaultDSQualityClass        PtpClockQualityClassType,
            iec6185093ConfigClockDefaultDSQualityAccuracy     PtpClockQualityAccuracyType,
            iec6185093ConfigClockDefaultDSMaximumStepsRemoved Unsigned32,
            iec6185093ConfigClockDefaultDSAccuracyAlert       Unsigned32
        }

    iec6185093ConfigClockDefaultDSInstanceIndex OBJECT-TYPE
        SYNTAX          PtpClockInstanceType
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "This object specifies the instance of the clock for this clock
                 type in the given domain."
        ::= { iec6185093ConfigClockDefaultDSEntry 1 }

    iec6185093ConfigClockDefaultDSClockType OBJECT-TYPE
        SYNTAX          PtpClockType
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "This object specifies the clock type as defined in the
                 textual convention description."
        ::= { iec6185093ConfigClockDefaultDSEntry 2 }

    iec6185093ConfigClockDefaultDSDomainNumber OBJECT-TYPE
        SYNTAX          PtpClockDomainType
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "This object specifies the domain number used to create a
                 logical group of PTP devices."
        ::= { iec6185093ConfigClockDefaultDSEntry 3 }

    iec6185093ConfigClockDefaultDSTwoStepFlag OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "This object specifies whether the two-step process is used."
        ::= { iec6185093ConfigClockDefaultDSEntry 4 }

    iec6185093ConfigClockDefaultDSClockIdentity OBJECT-TYPE
        SYNTAX          PtpClockIdentity
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the defaultDS clockIdentity member."
        ::= { iec6185093ConfigClockDefaultDSEntry 5 }

    iec6185093ConfigClockDefaultDSPriority1 OBJECT-TYPE
        SYNTAX          Unsigned32 (0..255)
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "This object specifies the defaultDS priority1 member."
        ::= { iec6185093ConfigClockDefaultDSEntry 6 }

    iec6185093ConfigClockDefaultDSPriority2 OBJECT-TYPE
        SYNTAX          Unsigned32 (0..255)
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "This object specifies the defaultDS priority2 member."
        ::= { iec6185093ConfigClockDefaultDSEntry 7 }

    iec6185093ConfigClockDefaultDSQualityClass OBJECT-TYPE
        SYNTAX          PtpClockQualityClassType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the defaultDS Quality Class."
        ::= { iec6185093ConfigClockDefaultDSEntry 8 }

    iec6185093ConfigClockDefaultDSQualityAccuracy OBJECT-TYPE
        SYNTAX          PtpClockQualityAccuracyType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the defaultDS Quality Accuracy."
        ::= { iec6185093ConfigClockDefaultDSEntry 9 }

    iec6185093ConfigClockDefaultDSMaximumStepsRemoved OBJECT-TYPE
        SYNTAX          Unsigned32 (2..255)
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "This object specifies the defaultDS Maximum Steps Removed."
        ::= { iec6185093ConfigClockDefaultDSEntry 10 }

    iec6185093ConfigClockDefaultDSAccuracyAlert OBJECT-TYPE
        SYNTAX          Unsigned32 (50..250000000)
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "This object specifies the defaultDS Accuracy Alert."
        ::= { iec6185093ConfigClockDefaultDSEntry 11 }

    iec6185093ConfigClockPortDSTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF Iec6185093ConfigClockPortDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "Table of information about the clock's portDS for a
                 particular domain."
        ::= { iec6185093ConfigClock 2 }

    iec6185093ConfigClockPortDSEntry OBJECT-TYPE
        SYNTAX          Iec6185093ConfigClockPortDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "A table entry that contains portDS information for
                 a single clock port."
        INDEX { iec6185093ConfigClockPortDSPortNumberIndex }
        ::= { iec6185093ConfigClockPortDSTable 1 }

    Iec6185093ConfigClockPortDSEntry ::= 
        SEQUENCE {
            iec6185093ConfigClockPortDSPortNumberIndex      PtpClockPortNumber,
            iec6185093ConfigClockPortDSPortIdentity         OCTET STRING,
            iec6185093ConfigClockPortDSlogAnnouncementInterval PtpClockIntervalBase2,
            iec6185093ConfigClockPortDSAnnounceRctTimeout   Integer32,
            iec6185093ConfigClockPortDSlogSyncInterval      PtpClockIntervalBase2,
            iec6185093ConfigClockPortDSMinDelayReqInterval  Integer32,
            iec6185093ConfigClockPortDSPeerDelayReqInterval Integer32,
            iec6185093ConfigClockPortDSDelayMech            PtpClockMechanismType,
            iec6185093ConfigClockPortDSEnabled              TruthValue 
        }

    iec6185093ConfigClockPortDSPortNumberIndex OBJECT-TYPE
        SYNTAX          PtpClockPortNumber
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "This object specifies the PTP portNumber associated with this
                 PTP port."
        ::= { iec6185093ConfigClockPortDSEntry 1 }

    iec6185093ConfigClockPortDSPortIdentity OBJECT-TYPE
        SYNTAX          OCTET STRING(SIZE(1..256))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the PTP clock port Identity."
        ::= { iec6185093ConfigClockPortDSEntry 2 }

    iec6185093ConfigClockPortDSlogAnnouncementInterval OBJECT-TYPE
        SYNTAX          PtpClockIntervalBase2
        UNITS           "Time Interval"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the Announce message transmission
                 interval associated with this clock port."
        ::= { iec6185093ConfigClockPortDSEntry 3 }

    iec6185093ConfigClockPortDSAnnounceRctTimeout OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the Announce receipt timeout associated
                 with this clock port."
        ::= { iec6185093ConfigClockPortDSEntry 4 }

    iec6185093ConfigClockPortDSlogSyncInterval OBJECT-TYPE
        SYNTAX          PtpClockIntervalBase2
        UNITS           "Time Interval"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the Sync message transmission interval."
        ::= { iec6185093ConfigClockPortDSEntry 5 }

    iec6185093ConfigClockPortDSMinDelayReqInterval OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the Delay_Req message transmission
                 interval."
        ::= { iec6185093ConfigClockPortDSEntry 6 }

    iec6185093ConfigClockPortDSPeerDelayReqInterval OBJECT-TYPE
        SYNTAX          Integer32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the Pdelay_Req message transmission
                 interval."
        ::= { iec6185093ConfigClockPortDSEntry 7 }

    iec6185093ConfigClockPortDSDelayMech OBJECT-TYPE
        SYNTAX          PtpClockMechanismType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the delay mechanism used.  If the clock
                 is an end-to-end clock, the value is e2e; if the
                 clock is a peer to-peer clock, the value is p2p."
        ::= { iec6185093ConfigClockPortDSEntry 8 }

    iec6185093ConfigClockPortDSEnabled OBJECT-TYPE
        SYNTAX          TruthValue
        MAX-ACCESS      read-write
        STATUS          current
        DESCRIPTION
                "True if port is enabled."
        DEFVAL { false }
        ::= { iec6185093ConfigClockPortDSEntry 9 }

    iec6185093ConfigTransportType OBJECT-TYPE
        SYNTAX  INTEGER {
                    udpipv4 (1),
                    udpipv6 (2),
                    ieee8023 (3),
                    devicenet (4),
                    controlnet (5),
                    profitnet (6),
                    ieee8021 (7),
                    unknown (65534)
                }
        MAX-ACCESS read-only
        STATUS     current 
        DESCRIPTION
                "This denotes the type of the interface. PTP port bears a seperate 
                 meaning than the one that are normally defined in interface mibs.
                 PTP can run over physical interfaces, VLAN identifiers or IVR
                 interfaces. Hence this object needs to be configured before making
                 this particular row as ACTIVE.
                 Only values UDP/IPv4, UDP/IPv6, IEEE802.3 are supported. IEEE8021 
                 corresponds to layer 2 Vlan."
        ::= { iec6185093ConfigClock 3 }

