-----------------------
-- IEEE C37.238 2017 -- 
-----------------------

    ieeeC37238StatClock  OBJECT IDENTIFIER
        ::= { ieeeC37238Status 1 }

    ieeeC37238StatClockCurrentDSTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF IeeeC37238StatClockCurrentDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "Table of information about the PTP clock currentDS for
                 all domains."
        ::= { ieeeC37238StatClock 1 }

    ieeeC37238StatClockCurrentDSEntry OBJECT-TYPE
        SYNTAX          IeeeC37238StatClockCurrentDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "A table entry that contains information about a single
                 PTP clock currentDS for a domain."
        REFERENCE
                "Section 8.2.2 ('currentDS data set member
                 specifications') of [IEEE-1588-2008]"
        INDEX { ieeeC37238StatClockCurrentDSInstanceIndex }
        ::= { ieeeC37238StatClockCurrentDSTable 1 }

    IeeeC37238StatClockCurrentDSEntry ::=
        SEQUENCE {
            ieeeC37238StatClockCurrentDSInstanceIndex    PtpClockInstanceType,
            ieeeC37238StatClockCurrentDSStepsRemoved     Unsigned32,
            ieeeC37238StatClockCurrentDSOffsetFromMaster PtpClockTimeInterval,
            ieeeC37238StatClockCurrentDSMeanPathDelay    PtpClockTimeInterval,
            ieeeC37238StatClockCurrentDSSyncStatus       Unsigned32,
            ieeeC37238StatClockCurrentDSSlavePort        Integer32,
            ieeeC37238StatClockCurrentDSClockTime        DisplayString
        }

    ieeeC37238StatClockCurrentDSInstanceIndex OBJECT-TYPE
        SYNTAX          PtpClockInstanceType
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "This object specifies the instance of the clock for this clock
                 type in the given domain."
        ::= { ieeeC37238StatClockCurrentDSEntry 1 }

    ieeeC37238StatClockCurrentDSStepsRemoved OBJECT-TYPE
        SYNTAX          Unsigned32
        UNITS           "Steps"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "The current clock dataset stepsRemoved value.

                 This object specifies the distance measured by the number of
                 boundary clocks between the local clock and the foreign master
                 as indicated in the stepsRemoved field of Announce messages."
        REFERENCE
                "Section 8.2.2.2 ('stepsRemoved') of [IEEE-1588-2008]"
        ::= { ieeeC37238StatClockCurrentDSEntry 2 }

    ieeeC37238StatClockCurrentDSOffsetFromMaster OBJECT-TYPE
        SYNTAX          PtpClockTimeInterval
        UNITS           "Time Interval"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the current clock dataset ClockOffset
                 value.  The value of the computation of the offset in time
                 between a slave and a master clock."
        REFERENCE
                "Section 8.2.2.3 ('currentDS.offsetFromMaster')
                 of [IEEE-1588-2008]"
        ::= { ieeeC37238StatClockCurrentDSEntry 3 }

    ieeeC37238StatClockCurrentDSMeanPathDelay OBJECT-TYPE
        SYNTAX          PtpClockTimeInterval
        UNITS           "Time Interval"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the current clock dataset
                 MeanPathDelay value.

                 The mean path delay between a pair of ports as measured by the
                 delay request-response mechanism."
        REFERENCE
                "Section 8.2.2.4 ('currentDS.meanPathDelay')
                 of [IEEE-1588-2008]"
        ::= { ieeeC37238StatClockCurrentDSEntry 4 }


    ieeeC37238StatClockCurrentDSSyncStatus OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "A value of 'DISABLED' (0) indicates that the ptp function is 
                 disabled. A value of 'FREERUN' (1) indicates that the device
                 is selected to be master. A value of 'SYNCING' (2) indicates
                 that the device is selected to be slave and is synchronizing
                 to grandmaster. A value of 'LOCKED' (3) indicates that the
                 device is synchronized to the grandmaster. A value of
                 'INITIALIZING' (4) indicates that the ptp function is
                 initializing."
        DEFVAL { 1 }
        ::= { ieeeC37238StatClockCurrentDSEntry 5 }

    ieeeC37238StatClockCurrentDSSlavePort OBJECT-TYPE
        SYNTAX          Integer32(1..255)
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "Indicate the port which is in the SLAVE state."
        ::= { ieeeC37238StatClockCurrentDSEntry 6 }

    ieeeC37238StatClockCurrentDSClockTime OBJECT-TYPE
        SYNTAX          DisplayString
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "Indicate the PTP Clock Time."
        ::= { ieeeC37238StatClockCurrentDSEntry 7 }

    ieeeC37238StatClockParentDSTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF IeeeC37238StatClockParentDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "Table of information about the PTP clock parentDS for
                 all domains."
        ::= { ieeeC37238StatClock 2 }

    ieeeC37238StatClockParentDSEntry OBJECT-TYPE
        SYNTAX          IeeeC37238StatClockParentDSEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "A table entry that contains information about a single
                 PTP clock parentDS for a domain."
        REFERENCE
                "Section 8.2.3 ('parentDS data set member specifications') of
                 [IEEE-1588-2008]"
        INDEX { ieeeC37238StatClockParentDSInstanceIndex }
        ::= { ieeeC37238StatClockParentDSTable 1 }

    IeeeC37238StatClockParentDSEntry ::=
        SEQUENCE {
            ieeeC37238StatClockParentDSInstanceIndex          PtpClockInstanceType,
            ieeeC37238StatClockParentDSParentPortIdentity     OCTET STRING,
            ieeeC37238StatClockParentDSGMClockIdentity        PtpClockIdentity,
            ieeeC37238StatClockParentDSGMClockPriority1       Unsigned32,
            ieeeC37238StatClockParentDSGMClockPriority2       Unsigned32,
            ieeeC37238StatClockParentDSGMClockQualityClass    PtpClockQualityClassType,
            ieeeC37238StatClockParentDSGMClockQualityAccuracy PtpClockQualityAccuracyType,
            ieeeC37238StatClockParentDSParentPortNumber       PtpClockPortNumber
        }

    ieeeC37238StatClockParentDSInstanceIndex OBJECT-TYPE
        SYNTAX          PtpClockInstanceType
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "This object specifies the instance of the clock for this clock
                 type in the given domain."
        ::= { ieeeC37238StatClockParentDSEntry 1 }

    ieeeC37238StatClockParentDSParentPortIdentity OBJECT-TYPE
        SYNTAX          OCTET STRING(SIZE(1..256))
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the value of portIdentity of the port on
                 the master that issues the Sync messages used in synchronizing
                 this clock."
        REFERENCE
                "Section 8.2.3.2 ('parentDS.parentPortIdentity') of
                 [IEEE-1588-2008]"
        ::= { ieeeC37238StatClockParentDSEntry 2 }

    ieeeC37238StatClockParentDSGMClockIdentity OBJECT-TYPE
        SYNTAX          PtpClockIdentity
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 identity."
        REFERENCE
                "Section 8.2.3.6 ('parentDS.grandmasterIdentity') of
                 [IEEE-1588-2008]"
        ::= { ieeeC37238StatClockParentDSEntry 3 }

    ieeeC37238StatClockParentDSGMClockPriority1 OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 priority1."
        REFERENCE
                "Section 8.2.3.8 ('parentDS.grandmasterPriority1') of
                 [IEEE-1588-2008]"
        ::= { ieeeC37238StatClockParentDSEntry 4 }

    ieeeC37238StatClockParentDSGMClockPriority2 OBJECT-TYPE
        SYNTAX          Unsigned32
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 priority2."
        REFERENCE
                "Section 8.2.3.9 ('parentDS.grandmasterPriority2') of
                 [IEEE-1588-2008]"
        ::= { ieeeC37238StatClockParentDSEntry 5 }

    ieeeC37238StatClockParentDSGMClockQualityClass OBJECT-TYPE
        SYNTAX          PtpClockQualityClassType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 quality class."
        REFERENCE
                "Section 8.2.3.7 ('parentDS.grandmasterClockQuality') of
                 [IEEE-1588-2008]"
        ::= { ieeeC37238StatClockParentDSEntry 6 }

    ieeeC37238StatClockParentDSGMClockQualityAccuracy OBJECT-TYPE
        SYNTAX          PtpClockQualityAccuracyType
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS grandmaster clock
                 quality accuracy."
        REFERENCE
                "Section 8.2.3.7 ('parentDS.grandmasterClockQuality') of
                 [IEEE-1588-2008]"
        ::= { ieeeC37238StatClockParentDSEntry 7 }

    ieeeC37238StatClockParentDSParentPortNumber OBJECT-TYPE
        SYNTAX          PtpClockPortNumber
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the parentDS PTP portNumber"
        ::= { ieeeC37238StatClockParentDSEntry 8 }

    ieeeC37238StatClockPortRunningTable OBJECT-TYPE
        SYNTAX          SEQUENCE OF IeeeC37238StatClockPortRunningEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "Table of information about the clock ports running datasets for
                 a particular domain."
        ::= { ieeeC37238StatClock 3 }

    ieeeC37238StatClockPortRunningEntry OBJECT-TYPE
        SYNTAX          IeeeC37238StatClockPortRunningEntry
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "A table entry that contains running dataset information
                 about a single clock port."
        INDEX { ieeeC37238StatClockPortRunningPortNumberIndex }
        ::= { ieeeC37238StatClockPortRunningTable 1 }

    IeeeC37238StatClockPortRunningEntry ::=
        SEQUENCE {
            ieeeC37238StatClockPortRunningPortNumberIndex    PtpClockPortNumber,
            ieeeC37238StatClockPortRunningState              INTEGER,
            ieeeC37238StatClockPortRunningPeerMeanPathDelay  PtpClockTimeInterval
        }

    ieeeC37238StatClockPortRunningPortNumberIndex OBJECT-TYPE
        SYNTAX          PtpClockPortNumber
        MAX-ACCESS      not-accessible
        STATUS          current
        DESCRIPTION
                "This object specifies the PTP portNumber associated with this
                 clock port."
        ::= { ieeeC37238StatClockPortRunningEntry 1 }

    ieeeC37238StatClockPortRunningState OBJECT-TYPE
        SYNTAX          INTEGER  {
                            initializing(1),
                            faulty(2),
                            disabled(3),
                            listening(4),
                            preMaster(5),
                            master(6),
                            passive(7),
                            uncalibrated(8),
                            slave(9),
                            TransmittingSynchronized(10),
                            ReceivingSynchronized (11)
                        }
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the port state returned by PTP engine:

                initializing
                faulty
                disabled
                listening
                preMaster
                master
                passive
                uncalibrated
                slave
                TransmittingSynchronized
                ReceivingSynchronized       "
        ::= { ieeeC37238StatClockPortRunningEntry 2 }

    ieeeC37238StatClockPortRunningPeerMeanPathDelay OBJECT-TYPE
        SYNTAX          PtpClockTimeInterval
        UNITS           "Time Interval"
        MAX-ACCESS      read-only
        STATUS          current
        DESCRIPTION
                "This object specifies the peer meanPathDelay."
        ::= { ieeeC37238StatClockPortRunningEntry 3 }

END
