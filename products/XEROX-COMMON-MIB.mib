      * FB  'hrFSBootable'
      * FS  'hrFSStorageIndex'
      * RO  'hrSWOSIndex'
      * RX  'hrSWRunIndex'
      * RN  'hrSWRunName'
      * RI  'hrSWRunID'
      * RF  'hrSWRunPath'
