      * SS  'hrStorageSize'
      * DX  'hrDeviceIndex'
      * DY  'hrDeviceType'
      * DD  'hrDeviceDescr'
      * DI  'hrDeviceID'
      * PI  'hrProcessorFrwID'
      * NI  'hrNetworkIfIndex'
      * KA  'hrDiskStorageAccess'
      * KM  'hrDiskStorageMedia'
      * KR  'hrDiskStorageRemoveble'
      * KC  'hrDiskStorageCapacity'
      * XX  'hrPartitionIndex'
      * XL  'hrPartitionLabel'
      * XI  'hrPartitionID'
