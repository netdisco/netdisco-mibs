      * UI  'xcmSecUserIndex'
      * UN  'xcmSecUserName'
      * UC  'xcmSecUserPasscode'
      * UW  'xcmSecUserPassword'
      * ZD  'sysDescr'
      * ZI  'sysObjectID'
      * ZS  'sysServices'
      * IX  'ifIndex'
      * ID  'ifDescr'
      * IY  'ifType'
      * IM  'ifMtu'
      * IS  'ifSpeed'
      * IP  'ifPhysAddress'
      * IZ  'ifSpecific'
      * ZA  'hrSystemDate'
      * ZL  'hrSystemInitialLoadDevice'
      * ZP  'hrSystemInitialLoadParameters'
      * ZM  'hrSystemMaxProcesses'
      * SX  'hrStorageIndex'
      * SY  'hrStorageType'
      * SD  'hrStorageDescr'
