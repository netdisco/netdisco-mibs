CISCO-DM-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-IDENTITY,
    OBJECT-TYPE,
    NOTIFICATION-TYPE,
    Unsigned32,
    Counter32,
    Integer32,
    Gauge32
        FROM SNMPv2-SMI
    MODULE-COMPLIANCE,
    OBJECT-GROUP,
    NOTIFICATION-GROUP
        FROM SNMPv2-CONF
    TEXTUAL-CONVENTION,
    TruthValue,
    RowStatus
        FROM SNMPv2-TC
    SnmpAdminString
        FROM SNMP-FRAMEWORK-MIB
    ifIndex
        FROM IF-MIB
    DomainId,
    DomainIdOrZero,
    FcNameId,
    FcNameIdOrZero,
    FcAddressId
        FROM CISCO-ST-TC
    vsanIndex,
    notifyVsanIndex
        FROM CISCO-VSAN-MIB
    cffFcFeElementName
        FROM CISCO-FC-FE-MIB
    ciscoMgmt
        FROM CISCO-SMI;


